# 2 part
my_dict = {"Evgeniy": 1999, "Danil": 2004, "Olesia": 2010}
print(my_dict)
print(my_dict["Olesia"])
print(my_dict.get("Eliza"))
my_dict["Nikita"] = 2001
my_dict.update({"Stepan": 2004, "Egor": 2015, "Sonya": 2005})
a = my_dict.pop("Sonya")
print(a)
print(my_dict)
